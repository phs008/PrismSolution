
int PropertyInstance::GetExpanded( )
{
	if (ENG_CAPI_PropertyInstance_GetExpanded)
	{
		return ENG_CAPI_PropertyInstance_GetExpanded(Owner->id );
	}
	else
		return 0;
}


void PropertyInstance::SetExpanded(  int value)
{
	if (ENG_CAPI_PropertyInstance_SetExpanded)
	{
		ENG_CAPI_PropertyInstance_SetExpanded(Owner->id,   value);
	}
	else
	{
		if (ENG_CAPI_Debugger_Log)
			ENG_CAPI_Debugger_Log("ENG_CAPI_PropertyInstance_SetExpanded is not Linked", 0, 0.0f );
	}
}




