
float PropertyGesture::GetConfidence( )
{
	if (ENG_CAPI_PropertyGesture_GetConfidence)
	{
		return ENG_CAPI_PropertyGesture_GetConfidence(Owner->id );
	}
	else
		return 0.0f;
}


void PropertyGesture::SetConfidence(  float value)
{
	if (ENG_CAPI_PropertyGesture_SetConfidence)
	{
		ENG_CAPI_PropertyGesture_SetConfidence(Owner->id,   value);
	}
	else
	{
		if (ENG_CAPI_Debugger_Log)
			ENG_CAPI_Debugger_Log("ENG_CAPI_PropertyGesture_SetConfidence is not Linked", 0, 0.0f );
	}
}




