
int PropertyKinectSkeleton::GetLeftHand( )
{
	if (ENG_CAPI_PropertyKinectSkeleton_GetLeftHand)
	{
		return ENG_CAPI_PropertyKinectSkeleton_GetLeftHand(Owner->id , GroupIndex);
	}
	else
		return 0;
}


void PropertyKinectSkeleton::SetLeftHand(  int value)
{
	if (ENG_CAPI_PropertyKinectSkeleton_SetLeftHand)
	{
		ENG_CAPI_PropertyKinectSkeleton_SetLeftHand(Owner->id,  GroupIndex,  value);
	}
	else
	{
		if (ENG_CAPI_Debugger_Log)
			ENG_CAPI_Debugger_Log("ENG_CAPI_PropertyKinectSkeleton_SetLeftHand is not Linked", 0, 0.0f );
	}
}




