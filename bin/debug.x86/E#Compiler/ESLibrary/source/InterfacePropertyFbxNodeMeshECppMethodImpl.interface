
string PropertyFbxNodeMesh::GetFbxMeshFile( )
{
	if (ENG_CAPI_PropertyFbxNodeMesh_GetFbxMeshFile)
	{
		return ENG_CAPI_PropertyFbxNodeMesh_GetFbxMeshFile(Owner->id );
	}
	else
		return "";
}


void PropertyFbxNodeMesh::SetFbxMeshFile(  const char * value)
{
	if (ENG_CAPI_PropertyFbxNodeMesh_SetFbxMeshFile)
	{
		ENG_CAPI_PropertyFbxNodeMesh_SetFbxMeshFile(Owner->id,   value);
	}
	else
	{
		if (ENG_CAPI_Debugger_Log)
			ENG_CAPI_Debugger_Log("ENG_CAPI_PropertyFbxNodeMesh_SetFbxMeshFile is not Linked", 0, 0.0f );
	}
}




