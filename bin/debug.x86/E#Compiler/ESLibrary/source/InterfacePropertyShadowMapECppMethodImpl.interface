
float PropertyShadowMap::GetShadowDarkness( )
{
	if (ENG_CAPI_PropertyShadowMap_GetShadowDarkness)
	{
		return ENG_CAPI_PropertyShadowMap_GetShadowDarkness(Owner->id );
	}
	else
		return 0.0f;
}


void PropertyShadowMap::SetShadowDarkness(  float value)
{
	if (ENG_CAPI_PropertyShadowMap_SetShadowDarkness)
	{
		ENG_CAPI_PropertyShadowMap_SetShadowDarkness(Owner->id,   value);
	}
	else
	{
		if (ENG_CAPI_Debugger_Log)
			ENG_CAPI_Debugger_Log("ENG_CAPI_PropertyShadowMap_SetShadowDarkness is not Linked", 0, 0.0f );
	}
}




